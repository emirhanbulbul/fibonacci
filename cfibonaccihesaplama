#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {
	int ilksayi=1;
	int ikincisayi=1;
	int sayac,sayaclimit;
	printf("Sana kac satir lazim ? ");
	scanf("%d",&sayaclimit);
printf("%d\n%d\n",ilksayi,ikincisayi);
for(sayac=0; sayac<sayaclimit-2; sayac++){
	int temp= ikincisayi;
	ikincisayi += ilksayi;
	ilksayi= temp;
	printf("%d\n",ikincisayi);

	
}
	return 0;
}
